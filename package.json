{
  "name": "gssh",
  "version": "0.1.0",
  "description": "SSH + Git author profile manager",
  "type": "module",
  "bin": {
    "gssh": "./bin/gssh"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "bin/"
  ],
  "engines": {
    "node": ">=18.0.0"
